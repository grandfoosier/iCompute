<!doctype html>
<html lang="en">
<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<!-- Bootstrap CSS -->
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

	<!-- Other CSS -->

	<title>iCompute</title>
<br>
</head>
<body>
<div class="row mx-3 justify-content-end">
  <a class="btn btn-link" href="/" role="button">Back</a>
</div>
<div>
<h2 style="text-align:center;"> Team scores, download CSV File</h2>
<br><br>
  <form method="get">
      <div style="text-align:center"> 
        <select class="btn btn-secondary btn-lg dropdown-toggle" name="year">
          <% for(var i=0; i<result.length; i++) { %>
          <option name="year" value="<%= result[i].year %>"><%= result[i].year %></option>
          <% } %>
        </select>
        &nbsp;&nbsp;&nbsp;
        <select class="btn btn-secondary btn-lg dropdown-toggle" name="grade">
          <option name="grade" value="4">4th grade</option>
          <option name="grade" value="5">5th grade</option>
        </select>
        &nbsp;&nbsp;&nbsp;
        <button class="btn btn-info btn-lg" type="submit" formaction="/getCSV"> Download CSV file for scores </button>
      </div>
  </form>
</div>


</body>

  
  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
